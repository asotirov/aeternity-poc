{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es2017",
    "types": [
      "node"
    ],
    "moduleResolution": "node",
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "inlineSources": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [
      "es2015"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
